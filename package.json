{
  "name": "campmajo-web",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "npm run build:static && npm run build:ts && npm run copy:views",
    "build:static": "webpack --config webpack.config.js && node optimizeImages.mjs",
    "build:ts": "tsc",
    "copy:views": "cp -R views dist/",
    "dev": "npm run webpack:watch & npm run ts:watch",
    "webpack:watch": "webpack --watch --config webpack.config.js && node optimizeImages.mjs",
    "ts:watch": "tsc --watch",
    "clean": "rm -rf dist node_modules package-lock.json",
    "start": "node dist/bin/www.js"
  },
  "dependencies": {
    "compression": "^1.7.4",
    "debug": "^4.3.7",
    "dotenv": "^16.4.5",
    "ejs": "^3.1.10",
    "express": "^4.20.0",
    "express-rate-limit": "^7.4.0",
    "helmet": "^7.1.0",
    "http-errors": "^2.0.0"
  },
  "devDependencies": {
    "@types/compression": "^1.7.5",
    "@types/debug": "^4.1.12",
    "@types/express": "^4.17.21",
    "@types/node": "^22.5.4",
    "autoprefixer": "^10.4.20",
    "css-loader": "^7.1.2",
    "file-loader": "^6.2.0",
    "imagemin": "^9.0.0",
    "mini-css-extract-plugin": "^2.9.1",
    "postcss": "^8.4.45",
    "sass": "^1.78.0",
    "sass-loader": "^16.0.1",
    "style-loader": "^4.0.0",
    "tailwindcss": "^3.4.10",
    "typescript": "^5.6.2",
    "webpack": "^5.94.0",
    "webpack-cli": "^5.1.4"
  }
}