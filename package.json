{
  "name": "campmajo-web",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "prebuild": "npm run clean && npm run update:deps",
    "build": "npm run build:ts && npm run copy:views",
    "build:ts": "tsc",
    "clean": "rm -rf dist",
    "copy:views": "cp -R views dist/",
    "start": "node dist/bin/www.js",
    "ts:watch": "tsc --watch",
    "update:deps": "npx npm-check-updates -u && npm install",
    "heroku-postbuild": "npm run build"
  },
  "dependencies": {
    "compression": "^1.8.1",
    "debug": "^4.4.3",
    "dotenv": "^17.2.3",
    "ejs": "^4.0.1",
    "express": "^5.2.1",
    "http-errors": "^2.0.1",
    "sharp": "^0.34.5",
    "typescript": "^5.9.3",
    "@types/compression": "^1.8.1",
    "@types/debug": "^4.1.12",
    "@types/express": "^5.0.6",
    "@types/http-errors": "^2.0.5",
    "@types/node": "^25.2.0"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.24",
    "copy-webpack-plugin": "^13.0.1",
    "css-loader": "^7.1.3",
    "file-loader": "^6.2.0",
    "imagemin": "^9.0.1",
    "mini-css-extract-plugin": "^2.10.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.6",
    "sass": "^1.97.3",
    "sass-loader": "^16.0.6",
    "style-loader": "^4.0.0",
    "tailwindcss": "^4.1.18",
    "ts-loader": "^9.5.4"
  },
  "engines": {
    "node": "20.x"
  }
}