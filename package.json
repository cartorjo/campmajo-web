{
  "name": "campmajo-web",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "prebuild": "npm run clean && npm run update:deps",
    "build": "npm run build:static && npm run build:ts && npm run copy:views",
    "build:static": "webpack --config webpack.config.js",
    "build:ts": "tsc",
    "postbuild:static": "node ./optimizeImages.mjs",
    "clean": "rm -rf dist node_modules",
    "copy:views": "cp -R views dist/",
    "dev": "npm-run-all --parallel webpack:watch ts:watch",
    "start": "node dist/bin/www.js",
    "ts:watch": "tsc --watch",
    "update:deps": "npx npm-check-updates -u && npm install",
    "webpack:watch": "webpack --watch --config webpack.config.js"
  },
  "dependencies": {
    "compression": "^1.7.4",
    "debug": "^4.3.7",
    "dotenv": "^16.4.5",
    "ejs": "^3.1.10",
    "express": "^5.0.0",
    "http-errors": "^2.0.0"
  },
  "devDependencies": {
    "@types/compression": "^1.7.5",
    "@types/debug": "^4.1.12",
    "@types/express": "^4.17.21",
    "@types/node": "^22.5.5",
    "autoprefixer": "^10.4.20",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^7.1.2",
    "file-loader": "^6.2.0",
    "imagemin": "^9.0.0",
    "mini-css-extract-plugin": "^2.9.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.47",
    "sass": "^1.78.0",
    "sass-loader": "^16.0.1",
    "style-loader": "^4.0.0",
    "tailwindcss": "^3.4.11",
    "ts-loader": "^9.5.1",
    "typescript": "^5.6.2",
    "webp-converter": "^2.3.3",
    "webpack": "^5.94.0",
    "webpack-cli": "^5.1.4"
  }
}
