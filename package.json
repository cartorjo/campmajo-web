{
  "name": "campmajo-web",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "prebuild": "npm run clean && npm run update:deps",
    "build": "npm run build:ts && npm run copy:views",
    "build:ts": "tsc",
    "clean": "rm -rf dist",
    "copy:views": "cp -R views dist/",
    "start": "node dist/bin/www.js",
    "ts:watch": "tsc --watch",
    "update:deps": "npx npm-check-updates -u && npm install",
    "heroku-postbuild": "npm run build"
  },
  "dependencies": {
    "compression": "^1.7.4",
    "debug": "^4.3.7",
    "dotenv": "^16.4.5",
    "ejs": "^3.1.10",
    "express": "^5.0.0",
    "http-errors": "^2.0.0",
    "sharp": "^0.33.5",
    "typescript": "^5.6.3"
  },
  "devDependencies": {
    "@types/compression": "^1.7.5",
    "@types/debug": "^4.1.12",
    "@types/express": "^5.0.0",
    "@types/node": "^22.7.6",
    "autoprefixer": "^10.4.20",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^7.1.2",
    "file-loader": "^6.2.0",
    "imagemin": "^9.0.0",
    "mini-css-extract-plugin": "^2.9.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.47",
    "sass": "^1.80.1",
    "sass-loader": "^16.0.2",
    "style-loader": "^4.0.0",
    "tailwindcss": "^3.4.14",
    "ts-loader": "^9.5.1"
  }
}
